<%= provide(:title, 'Visualizar Cliente') %>
<div class="row">
  <div class="col-md-6 col-md-offset-3">

    <p>
      <strong>Nombre:</strong>
      <%= @cliente.nombre %>
    </p>

    <p>
      <strong>Dirección:</strong>
      <%= @cliente.direccion %>
    </p>

    <p>
      <strong>Dirección Fiscal:</strong>
      <%= @cliente.direccionFiscal %>
    </p>

    <p>
      <strong>Fax:</strong>
      <%= @cliente.fax %>
    </p>

    <p>
      <strong>Rif:</strong>
      <%= @cliente.rif %>
    </p>

    <p>
      <strong>Url:</strong>
      <%= @cliente.url %>
    </p>

    <%= link_to 'Editar', edit_cliente_path(@cliente),  class:"btn btn-default
btn-lg active", role:"button"  %>
    <%= link_to 'listar Clientes', clientes_path , class:"btn btn-default
                btn-lg active", role:"button"  %>
  </div>
</div>