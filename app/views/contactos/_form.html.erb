<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for([@cliente,@contacto]) do |f| %>
        <%= render 'shared/error_messages', object: f.object %>

        <div class="field">
          <%= f.label :nombre %><br>
          <%= f.text_field :nombre,class: 'form-control' %>
        </div>

        <div class="field">
          <%= f.label :email %><br>
          <%= f.email_field :email,class: 'form-control' %>
        </div>

        <div class="field">
          <%= f.label :telefonoCelular ,'Teléfono Celular'%><br>
          <%= f.telephone_field :telefonoCelular,class: 'form-control' %>
        </div>

        <div class="field">
          <%= f.label :telefonoOficina, 'Teléfono Oficina' %><br>
          <%= f.telephone_field :telefonoOficina ,class: 'form-control' %>
        </div>

        <div class="actions">
          <%= f.submit "#{nameButton}", class: "btn btn-primary"%>
        </div>
    <% end %>
  </div>
</div>